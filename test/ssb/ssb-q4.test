        LO_ORDTOTALPRICE        UINTEGER,
        LO_DISCOUNT             USMALLINT,
        LO_REVENUE              UINTEGER,
        LO_SUPPLYCOST           UINTEGER,
        LO_TAX                  USMALLINT,
        LO_COMMITDATE           UINTEGER,
        LO_SHIPMODE             VARCHAR
)

